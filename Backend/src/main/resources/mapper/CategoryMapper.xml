<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >



<mapper namespace="pri.chaofan.mineblog.dao.CategoryMapper">

    <resultMap id="categoryMap" type="pri.chaofan.mineblog.pojo.Category">
        <result column="name_category" property="name"/>
    </resultMap>


    <select id="selectAll" resultMap="categoryMap">
        select * from tb_categories;
    </select>


</mapper>